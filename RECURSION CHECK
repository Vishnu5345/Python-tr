#recursion Example
def abc(n):
    if n==1:
        return 2
    b=abc(n-1)
    print("Hai",end=' \n')
    return b+1
print(abc(5))


#recursion Example
def abc(a):
    if a==4:
        return
    a+=1
    abc(a)
    print("CSE",end=' ')
    abc(a)
    print("MECH",end=' ')

def main():
    num="12"
    for a in num:
        print()
        abc(int(a))
main()






def raju(a):
    if(a==1):
        return 
    raju(a-1)
    print("Hai")
    raju(a-1)
raju(5)

